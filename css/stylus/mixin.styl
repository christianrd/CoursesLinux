/*
*   *** MIXINS  ***
*/
transition(value)
  -webkit-transition all ease value
  -moz-transition all ease value
  -o-transition all ease value
  transition all ease value

border-radius(value)
  -webkit-border-radius value
  -moz-border-radius value
  border-radius value